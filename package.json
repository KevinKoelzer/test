  "scripts": {
    "ng": "ng",
    "ben": "nodemon --exec ts-node --fast second/ben.ts",
    "ngrok": "ngrok http 4200 --host-header=localhost:8080",
    "http": "cd dist && http-server -p 8080",
    "check": "npm ls --depth=0",
    "analyzer": "webpack-bundle-analyzer dist/stats.json",
    "start": "ng serve",
    "deploy": "ng lint --fix && ng build -sm -vc=false -prod --build-optimizer && firebase deploy --only hosting",
    "universal": "ng build --prod && ng build --prod --app ssr --output-hashing=false && webpack --config webpack.server.config.js --progress --colors && node dist/server.js",
    "tester": "nodemon tester.js",
    "build": "ng build --prod --color ",
    "dev": "nodemon --exec 'ts-node --cache-directory .tscache' ./server.ts",
    "starter": "ts-node --fast ./second/ben.ts",
    "faker": "ng build --prod && firebase serve",
    "test": "ng test",
    "lint": "tslint \"src/**/*.ts\" --fix",
    "e2e": "ng e2e"
  },
